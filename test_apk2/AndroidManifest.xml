<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.test_apk2"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="21" />
    
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity android:name="com.example.test_apk2.badge_count.BadgeCountActivity"/>
        <activity android:name="com.example.test_apk2.use_ime_range.UseImeRangeActivity"
            android:windowSoftInputMode="adjustResize|stateHidden" />
        <activity android:name="com.example.test_apk2.use_ime_range.UseImeRangeWithGridActivity"
            android:windowSoftInputMode="adjustResize|stateHidden" />
        <activity android:name="com.example.test_apk2.use_ime_range.ImeWithViewPagerActivity"
            android:windowSoftInputMode="adjustResize|stateHidden" />
        <activity android:name="com.example.test_apk2.check_ime_actived.CheckImeActivedActivity"
            android:configChanges="orientation|screenSize"
            android:windowSoftInputMode="adjustResize|stateHidden" />
        <activity android:name="com.example.test_apk2.floating_action_button.FloatActionButtonActivity"/>
        <activity android:name="com.example.test_apk2.floating_action_button.FloatActionButtonActivity2"
            android:windowSoftInputMode="adjustResize|stateHidden" />
        <activity android:name="com.example.test_apk2.floating_action_button.FloatActionButtonActivity3"/>
        <activity android:name="com.example.test_apk2.list_animation.ListAnimationActivity"
            android:theme="@style/CustomActionBarTheme" />
        <activity android:name="com.example.test_apk2.view_pager.ViewPagerActivity"/>
        <activity android:name="com.example.test_apk2.ramdom_number.RandomNumberActivity"/>
        <activity android:name="com.example.test_apk2.ramdom_number.RandomNumberActivity2"/>
        <activity android:name="com.example.test_apk2.test_HttpClient.LoadImageViewActivity"/>
        <activity android:name="com.example.test_apk2.test_HttpClient.UseWebViewActivity"/>
    </application>

</manifest>
